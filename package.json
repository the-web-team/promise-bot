  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "2.0.1",
    "@prisma/client": "5.3.1",
    "discord-api-types": "0.37.56",
    "discord.js": "14.13.0",
    "discordx": "11.7.6",
    "openai": "4.8.0",
    "reflect-metadata": "0.1.13",
    "yaml": "2.3.2"
  },
  "devDependencies": {
    "@swc-node/register": "1.6.7",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.86",
    "@types/jest": "29.5.5",
    "@types/node": "20.6.2",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "esbuild": "0.19.3",
    "esbuild-register": "3.5.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-security": "1.7.1",
    "jest": "29.7.0",
    "nodemon": "3.0.1",
    "prisma": "5.3.1",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect 0.0.0.0' pnpx vite-node src/server.ts",
    "test": "jest",
    "start": "pnpx vite-node src/server.ts",
    "lint": "next lint --dir src",
    "prisma": "pnpx prisma",
    "studio": "pnpm prisma studio --browser none",
    "generate": "pnpm prisma generate",
    "migrate": "pnpm docker:migrate && pnpm generate",
    "migrate:prod": "pnpm prisma migrate deploy",
    "docker:install": "pnpm install --frozen-lockfile",
    "docker:dev": "pnpm docker:install && pnpm dev",
    "docker:migrate": "docker compose exec bot pnpx prisma migrate dev"
  }
