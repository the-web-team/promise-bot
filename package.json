{
  "name": "bot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^1.5.0",
    "@prisma/client": "^4.11.0",
    "discord.js": "14.7.1",
    "discordx": "^11.1.12",
    "reflect-metadata": "0.1.13",
    "yaml": "2.2.1"
  },
  "devDependencies": {
    "@swc-node/register": "1.6.2",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.38",
    "@types/jest": "29.4.0",
    "@types/node": "18.14.6",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "esbuild": "0.17.11",
    "esbuild-register": "3.4.2",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-security": "1.7.1",
    "jest": "29.5.0",
    "nodemon": "2.0.21",
    "prisma": "^4.11.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect 0.0.0.0' pnpx vite-node src/server.ts",
    "test": "jest",
    "start": "pnpx vite-node src/server.ts",
    "lint": "next lint --dir src",
    "prisma": "pnpx prisma",
    "studio": "pnpm prisma studio --browser none",
    "generate": "pnpm prisma generate",
    "migrate": "pnpm docker:migrate && pnpm generate",
    "migrate:prod": "pnpm prisma migrate deploy",
    "docker:install": "pnpm install --frozen-lockfile",
    "docker:dev": "pnpm docker:install && pnpm dev",
    "docker:migrate": "docker compose exec bot pnpx prisma migrate dev"
  }
}
