{
  "name": "bot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "1.7.0",
    "@prisma/client": "4.12.0",
    "discord-api-types": "0.37.37",
    "discord.js": "14.9.0",
    "discordx": "11.7.4",
    "reflect-metadata": "0.1.13",
    "yaml": "2.2.1"
  },
  "devDependencies": {
    "@swc-node/register": "1.6.3",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.44",
    "@types/jest": "29.5.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "esbuild": "0.17.15",
    "esbuild-register": "3.4.2",
    "eslint": "8.37.0",
    "eslint-config-next": "13.2.4",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-security": "1.7.1",
    "jest": "29.5.0",
    "nodemon": "2.0.22",
    "prisma": "4.12.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.3"
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect 0.0.0.0' pnpx vite-node src/server.ts",
    "test": "jest",
    "start": "pnpx vite-node src/server.ts",
    "lint": "next lint --dir src",
    "prisma": "pnpx prisma",
    "studio": "pnpm prisma studio --browser none",
    "generate": "pnpm prisma generate",
    "migrate": "pnpm docker:migrate && pnpm generate",
    "migrate:prod": "pnpm prisma migrate deploy",
    "docker:install": "pnpm install --frozen-lockfile",
    "docker:dev": "pnpm docker:install && pnpm dev",
    "docker:migrate": "docker compose exec bot pnpx prisma migrate dev"
  }
}
