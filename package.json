{
  "name": "bot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "1.7.1",
    "@prisma/client": "5.0.0",
    "discord-api-types": "0.37.50",
    "discord.js": "14.11.0",
    "discordx": "11.7.6",
    "openai": "3.3.0",
    "reflect-metadata": "0.1.13",
    "yaml": "2.3.1"
  },
  "devDependencies": {
    "@swc-node/register": "1.6.6",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.70",
    "@types/jest": "29.5.3",
    "@types/node": "20.4.4",
    "@typescript-eslint/eslint-plugin": "6.1.0",
    "@typescript-eslint/parser": "6.1.0",
    "esbuild": "0.18.15",
    "esbuild-register": "3.4.2",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-security": "1.7.1",
    "jest": "29.6.1",
    "nodemon": "3.0.1",
    "prisma": "5.0.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  },
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect 0.0.0.0' pnpx vite-node src/server.ts",
    "test": "jest",
    "start": "pnpx vite-node src/server.ts",
    "lint": "next lint --dir src",
    "prisma": "pnpx prisma",
    "studio": "pnpm prisma studio --browser none",
    "generate": "pnpm prisma generate",
    "migrate": "pnpm docker:migrate && pnpm generate",
    "migrate:prod": "pnpm prisma migrate deploy",
    "docker:install": "pnpm install --frozen-lockfile",
    "docker:dev": "pnpm docker:install && pnpm dev",
    "docker:migrate": "docker compose exec bot pnpx prisma migrate dev"
  }
