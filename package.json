{
  "name": "bot",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "discord.js": "^14.3.0",
    "discordx": "^11.1.12",
    "reflect-metadata": "^0.1.13",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@swc-node/register": "1.5.1",
    "@swc/cli": "0.1.57",
    "@swc/core": "1.3.0",
    "@types/jest": "29.0.1",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "esbuild": "0.15.7",
    "esbuild-register": "3.3.3",
    "eslint": "8.22.0",
    "eslint-config-next": "12.3.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-security": "1.5.0",
    "jest": "29.0.3",
    "nodemon": "2.0.19",
    "prisma": "^4.3.1",
    "ts-jest": "29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "dev": "nodemon --watch . -e ts,js,json,yml --exec 'node -r @swc-node/register src/server.ts'",
    "docker-dev": "pnpm i && pnpm dev",
    "test": "jest",
    "start": "node -r @swc-node/register src/server.ts",
    "lint": "eslint",
    "prisma": "npx --yes prisma",
    "studio": "pnpm prisma studio --browser none",
    "generate": "pnpm prisma generate",
    "migrate": "pnpm docker:migrate && yarn generate",
    "migrate:prod": "pnpm prisma migrate deploy",
    "docker:dev": "pnpm && NODE_OPTIONS='--inspect 0.0.0.0' pnpm dev",
    "docker:migrate": "docker compose exec bot npx prisma migrate dev"
  }
}
